{
  "name": "prisma",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "pnpm db:start && pnpm prisma:migrate && node --watch src/index.js",
    "prisma:migrate": "exec prisma migrate dev --name init",
    "prisma:studio": "exec prisma studio",
    "db:start": "exec docker compose up -d",
    "db:stop": "exec docker compose stop",
    "db:delete": "exec docker compose down"
  },
